 /dts-v1/;
 /plugin/;
#include "../imx6ul-pinfunc.h"

 / {
     fragment@0 {
         target = <&i2c1>;
         __overlay__ { 
            #address-cells = <1>;
            #size-cells = <0>;    		
            mpu6050@68 {
                compatible = "mpu6050,INV_MPU6050";
                reg = <0x68>;
                position = <1>;
            };          
         };
     };

 };
