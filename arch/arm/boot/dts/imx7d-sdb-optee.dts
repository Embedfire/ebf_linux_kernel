#include "imx7d-sdb.dts"

&ocram {
	reg = <0x00905000 0x1b000>;
};

&ocrams_ddr {
	reg = <0x00904000 0x1000>;
};

&ocrams {
	/* 0x180000 are used by optee */
	reg = <0x00900000 0x4000>;
	clocks = <&clks IMX7D_OCRAM_CLK>;
};
